#!/bin/sh
# -*-tcl-*-
# The next line restarts using tclsh \
exec tclsh "$0" "$@"

set module [lindex $argv 0]

if {$module eq ""} {
    puts "Usage: grabit module"
}

set s ~/mars/lib/marsutil
set s2 ~/mars/docs/mann
set d ~/github/athena-mars

file copy -force $s/$module.tcl $d/lib/marsutil

if {[file exist $s/$module.test]} {
    file copy -force $s/$module.test $d/test/marsutil    
}

file copy -force $s2/$module.ehtml $d/docs/man1

